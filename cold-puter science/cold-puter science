import java.util.*;
public class cold{
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int counter =0;
        int n = input.nextInt();
        for(int i=0; i<n; i++){
            if(input.nextInt() < 0){
                counter++;
            }
        }
        System.out.println(counter);
    }
}
